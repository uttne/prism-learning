<UserControl x:Class="Module.Views.Startup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <StackPanel>
        <TextBlock Text="Startup" FontSize="48" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Button Command="{Binding GoNaviCommand}" Content="Go Navi"/>
    </StackPanel>
</UserControl>